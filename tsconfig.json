{
	"compilerOptions": {
		"plugins": [{
			"transform": "ts-macros",
			"noComptime": true
		}],
		"lib": ["ESNext", "DOM"],
		"target": "ESNext",
		"module": "ESNext",
		"moduleDetection": "force",
		"jsx": "react-jsx",
		"allowJs": true,
		"moduleResolution": "bundler",
		"allowImportingTsExtensions": true,
		"verbatimModuleSyntax": true,
		"noEmit": true,
		"strict": true,
		"skipLibCheck": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"noPropertyAccessFromIndexSignature": false,
		"baseUrl": ".",
		"paths": {
			"@common": ["src/common/Common.ts"],
			"@server/class": ["src/server/class/Class.ts"],
			"@server/router": ["src/server/router/Router.ts"],
			"@server/type": ["src/server/type/Type.ts"],
			"@server/util": ["src/server/util/Util.ts"],
			"@web/lib": ["src/web/lib/Lib.ts"],
			"@web/constant": ["src/web/constant/Constant.ts"],
			"@web/components": ["src/web/components/Components.ts"],
			"@web/hook": ["src/web/hook/Hook.ts"],
			"@web/page": ["src/web/page/Page.ts"]
		}
	}
}